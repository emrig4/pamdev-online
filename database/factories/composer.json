{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/airondev/laravel-slack-alerts.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/airondev/laravel-paystack-subscription.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/AironDev/paysub.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/airondev/cashier-paystack.git"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "airondev/laravel-paystack-subscription": "dev-main",
        "airondev/laravel-slack-alerts": "dev-dev",
        "binshops/laravel-blog": "^9.2",
        "caffeinated/themes": "^7.2",
        "cviebrock/eloquent-sluggable": "^8.0",
        "digitaldream/laravel-seo-tools": "^1.3",
        "diglactic/laravel-breadcrumbs": "^7.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^8.40",
        "laravel/jetstream": "^2.3",
        "laravel/legacy-factories": "^1.1",
        "laravel/sanctum": "^2.6",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^2.6.2",
        "maatwebsite/excel": "^3.1",
        "mckenziearts/laravel-notify": "^2.3",
        "mhmiton/laravel-modules-livewire": "^1.2",
        "nwidart/laravel-modules": "^8.2",
        "spatie/laravel-permission": "^5.5",
        "stichoza/google-translate-php": "^4.1",
        "unicodeveloper/laravel-paystack": "^1.0",
        "webpatser/laravel-uuid": "^4.0",
        "wikimedia/composer-merge-plugin": "^2.0",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Modules\\": "app/Modules/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "php artisan optimize",
            "php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate --ansi",
            "php -r \"file_exists('database/database.sql') || copy('database/database.sqlite', 'database/database.sql');\""

        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "require": [
                "app/Modules/*/composer.json",
                "themes/*/composer.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
